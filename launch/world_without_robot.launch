<launch>
  <!--rosparam file="$(find multi_legged_control)/config/gazebo_ros_control_params.yaml" command="load"/-->

  <!-- these are the arguments you can pass this launch file, for example paused:=true -->
  <arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <arg name="debug" default="false"/>

  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find multi_legged_gazebo)/launch/empty_world.launch">
    <arg name="world_name" value="$(find multi_legged_gazebo)/worlds/multi_legged.world"/>
    <arg name="debug" value="$(arg debug)" />
    <arg name="gui" value="$(arg gui)" />
    <arg name="paused" value="$(arg paused)"/>
    <arg name="use_sim_time" value="$(arg use_sim_time)"/>
    <arg name="headless" value="$(arg headless)"/>
  </include>

  <!--<node pkg="multi_legged_tools" name="save_each_torque_data" type="save_each_torque_data.py"/>
  <node pkg="multi_legged_tools" name="save_each_grf_data" type="save_each_grf_data.py"/>
  <node pkg="multi_legged_tools" name="save_robot_position" type="save_robot_position.py"/>-->

</launch>
